{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ng-particle-wave": {
      "projectType": "library",
      "root": "projects/ng-particle-wave",
      "sourceRoot": "projects/ng-particle-wave/src",
      "prefix": "",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/ng-particle-wave/tsconfig.lib.json",
            "project": "projects/ng-particle-wave/ng-package.json"
          },
          "configurations": {
            "production": {

            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ng-particle-wave/src/test.ts",
            "tsConfig": "projects/ng-particle-wave/tsconfig.spec.json",
            "karmaConfig": "projects/ng-particle-wave/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ng-particle-wave/tsconfig.lib.json",
              "projects/ng-particle-wave/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }},
  "defaultProject": "ng-particle-wave"
}
